<svg
   xmlns:svg="http://www.w3.org/2000/svg"
   xmlns:xlink="http://www.w3.org/1999/xlink"
   xmlns:plot="http://www.svgplot.org"
   width="300"
   height="300"
   version="1.0"
   plot:layout="2 2" >
   
   <g plot:type="box" plot:width="200" plot:height="100" plot:background="#EEE" plot:plotBackground="#CCC">
      <g plot:type="title">Jason Graph</g>
      <g plot:type="axes" xrange="-4 4" yrange="-2 2" xaxis="bottom" yaxis="left" xscale="linear" yscale="linear">
         <g plot:type="yAxis" linerange="-1.5 1.5" stroke-thickness="2" stroke-color="red" stroke-dashing="1 2 1" />
         <g plot:type="yTicks" location="-2 -1 0 1 2" length="2" />
         <g plot:type="yTicks" spacing=".2" length="1" />
         <g plot:type="yStubs" spacing=".5" />
         <g plot:type="xStubs">
            <g plot:type="stub" location="-3.14">-pi</label>
            <g plot:type="stub" location="-1.7">-pi/2</label>
            <g plot:type="stub" location="0">0</label>
            <g plot:type="stub" location="-1.7">pi/2</label>
            <g plot:type="stub" location="3.14">pi</label>
         </g>
         <g plot:type="function" func="sin(x)" xmin="-3.14" xmax="3.14" stroke-style="black" stroke-thickness="2" />
         <g plot:type="line" xdata="1 2 3 4 5 6" ydata="1 4 9 25 36" stroke-style="red" />
         <g plot:type="scatter" xdata="1 2 3 4 5 6" ydata="1 4 9 25 36" stroke-style="blue" />
      </g>  <!-- Another one that's 10x as big laid right on top -->
      <g plot:type="axes xrange="-40 40" yrange="-20 20" xposition="0" yposition="0">
         <g plot:type="yAxis" linerange="-15 15" stroke-thickness="2" stroke-color="red" stroke-dashing="1 2 1" />
         <g plot:type="yTicks" location="-20 -10 0 10 20" length="2" />
         <g plot:type="yTicks" spacing="2" length="1" />
         <g plot:type="yStubs" spacing="5"></yLabels>
         <g plot:type="xStubs" plot:location="auto">
            <!--
            <g plot:type="stub" location="-31.4"><latex>$-10 pi$</latex></label>
            <g plot:type="stub" location="-17"><latex>$-5 pi$</latex></label>
            <g plot:type="stub" location="0"><latex>$0$</latex></label>
            <g plot:type="stub" location="-17"><latex>$5 pi$</latex></label>
            <g plot:type="stub" location="31.4"><latex>$10 pi$</latex></label>
            -->
         </g>
         <g plot:type="function" func="5 sin(x/5)" xmin="-3.14" xmax="3.14" stroke-style="black" stroke-thickness="2" />
      </g>
   </g>
   
   <!-- When an array of plots share the same x-range and x-axis, you need to define it elsewhere. -->
</svg>
